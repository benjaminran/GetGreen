<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <gradient
        android:centerX="0"
        android:centerY="0"
        android:startColor="@color/transparentgray"
        android:centerColor="@color/transparentgreen"
